---
import SectionPage from "../../layouts/SectionPage.astro";
import Form from "../../components/Form.astro";
import Button from "../../components/Button.astro";
import { allForms } from "../../lib/settings";

const form = allForms.find(
  (item) => item.id === "7cc5ce68-2db2-4bda-a574-2d6680ae7f68"
);
---

<SectionPage
  currentPage={"c27d6639-bd40-454d-b210-63ca75b0dbbd"}
  localNavSection="Utility"
>
  <div class="center-items">
    <Button href="/contact#forms">See All Forms</Button>
  </div>
  <Form formData={form} />
</SectionPage>

<style>
  .center-items {
    margin-bottom: 3rem;
  }
</style>
